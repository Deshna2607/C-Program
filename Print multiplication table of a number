#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Enter a number to print its table: ";
    cin >> n;

    cout << "Multiplication Table of " << n << ":" << endl;
    cout << "---------------------------" << endl;

    // Loop from 1 to 10
    for (int i = 1; i <= 10; i++) {
        // Output format: n * i = result
        cout << n << " * " << i << " = " << n * i << endl;
    }

    return 0;
}
