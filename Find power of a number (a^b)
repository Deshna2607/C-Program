#include <iostream>
#include <cmath> // Required for pow()
using namespace std;

int main() {
    double base, exponent, result;

    cout << "Enter base and exponent: ";
    cin >> base >> exponent;

    // Calculate base^exponent
    result = pow(base, exponent);

    cout << base << "^" << exponent << " = " << result << endl;

    return 0;
}
